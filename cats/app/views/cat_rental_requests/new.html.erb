<form action="<%= cat_rental_requests_url%>" method="post">
  <input type="hidden" name="cat_request[status]" value="PENDING">
  <select name="cat_request[cat_id]">
    <option disabled selected>- Select Cat -</option>
    <% @cats.each do |cat| %>
      <option value="<%=cat.id %>"><%= cat.name %></option>
    <% end %>
  </select>

  <h1>Start date</h1>
  <input type="date" name="cat_request[start_date]" value="">

  <h1>End date</h1>
  <input type="date" name="cat_request[end_date]" value="">
  <input type='submit'></input>
</form>
